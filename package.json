{
  "name": "datavisyn-scatterplot",
  "version": "1.0.0-SNAPSHOT",
  "description": "a canvas based scatterplot implementation",
  "homepage": "https://datavisyn.io",
  "author": {
    "name": "datavisyn GmbH",
    "email": "contact@datavisyn.io",
    "url": "https://datavisyn.io"
  },
  "contributors": [
    {
      "name": "Samuel Gratzl",
      "email": "samuel.gratzl@datavisyn.io"
    }
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/datavisyn/datavisyn-scatterplot/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datavisyn/datavisyn-scatterplot.git"
  },
  "main": "build/scatterplot.js",
  "jsnext:main": "build/scatterplot.js",
  "module": "build/scatterplot.js",
  "unpkg": "build/scatterplot.js",
  "types": "build/typings/index.d.ts",
  "files": [
    "build",
    "!build/docs",
    "!build/demo",
    "src/**/*.ts",
    "src/assets/**/",
    "src/**/*.tsx",
    "src/**/*.scss"
  ],
  "scripts": {
    "clean": "rimraf build dist src/**/*.map src/**/*.js tests/**/*.js tests/**/*.map",
    "compile": "tsc",
    "lint": "tslint --project tsconfig.json -c tslint.json && stylelint src/**/*.scss",
    "docs": "typedoc --out ./build/docs/ src tsd.d.ts",
    "prebuild": "npm run clean && (node -e \"process.exit(process.env.PHOVEA_SKIP_TESTS === undefined?1:0)\" || npm run test)",
    "pretest": "npm run clean && npm run compile",
    "test": "node -e \"require('fs').existsSync('./tests')?0:1\" || karma start",
    "posttest": "npm run lint",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "build:dev": "webpack --mode development --devtool source-map",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch --devtool source-map",
    "start": "webpack-dev-server --mode development --devtool source-map",
    "preversion": "npm test",
    "prepublishOnly": "npm run build",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "release:pre": "npm version prerelease && npm publish --tag=next && git push --follow-tags",
    "predist": "npm run build && npm run docs",
    "dist": "mkdirp dist && cd build && tar cvzf ../dist/datavisyn-scatterplot.tar.gz --exclude \"docs\" * && cd .."
   },
  "dependencies": {
    "@types/d3-format": "^1.3.0",
    "@types/d3-scale": "^2.0.0",
    "@types/d3-shape": "^1.2.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.7",
    "cache-loader": "^1.2.2",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "jasmine": "^3.1.0",
    "jasmine-promises": "^0.4.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0",
    "raw-loader": "^1.0.0-beta.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.0",
    "thread-loader": "^1.1.5",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-consistent-codestyle": "^1.13.0",
    "tslint-eslint-rules": "^5.2.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "@types/node": "^10.1.0",
    "@types/d3-array": "^1.2.1",
    "@types/d3-brush": "^1.0.7",
    "@types/d3-drag": "^1.2.0",
    "@types/d3-polygon": "^1.0.6",
    "@types/d3-quadtree": "^1.0.5",
    "@types/d3-axis": "^1.0.10",
    "@types/d3-zoom": "^1.7.0",
    "d3-array": "^1.2.1",
    "d3-axis": "^1.0.8",
    "d3-brush": "^1.0.4",
    "d3-drag": "^1.2.1",
    "d3-format": "^1.3.0",
    "d3-polygon": "^1.0.3",
    "d3-quadtree": "^1.0.3",
    "d3-scale": "^2.0.0",
    "d3-selection": "^1.3.0",
    "d3-shape": "^1.2.0",
    "d3-zoom": "^1.7.1",
    "eventemitter3": "3.1.0"
  }
}
